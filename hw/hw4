function extractLetter()
List= {{'M','A','T','L','A','B'}, {' '}, {'i','s'}, {' '}, {'a'}, {' '}, {'s','t','r','a','n','g','e'}, {', '}, {'b','u','t',' '}, {'p','o','p','u','l','a','r'}, {' '}, {'p','r','o','g','r','a','m','m','i','n','g',' ','l','a','n','g','u','a','g','e'} };
lengthlist=length(List);
    for i=1:lengthlist
    x=length(List{i});
    for z=1:x
        fprintf(List{i}{z})
    end
    end
    
